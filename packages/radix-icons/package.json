{
	"name": "@radix-ui/react-icons",
	"version": "1.3.2",
	"license": "MIT",
	"description": "Radix UI React Icon Set",
	"source": "./src/index.tsx",
	"type": "module",
	"main": "./src/index.tsx",
	"module": "./src/index.tsx",
	"exports": {
		".": "./src/index.tsx",
		"./icons/*.svg": "./icons/*.svg",
		"./*": "./src/*.tsx"
	},
	"files": [
		"dist",
		"src",
		"icons",
		"manifest.json"
	],
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"module": "dist/index.js",
		"main": "dist/index.js",
		"types": "dist/index.d.ts",
		"exports": {
			".": {
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.js"
				},
				"require": {
					"types": "./dist/csj/index.d.ts",
					"default": "./dist/cjs/index.js"
				}
			},
			"./icons/*.svg": "./icons/*.svg",
			"./*": {
				"import": {
					"types": "./dist/*.d.ts",
					"default": "./dist/*.js"
				},
				"require": {
					"types": "./dist/csj/*.d.ts",
					"default": "./dist/cjs/*.js"
				}
			}
		}
	},
	"scripts": {
		"generate-src": "generate-icon-lib --file=uvCPWGs49VP4oslkOtVcCu && mv ./icons/15/* ./icons && rmdir ./icons/15 && cd ./icons && zip -r ../../../radix-icons.zip ./",
		"build": "rimraf dist && pnpm build:esm && pnpm build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --project tsconfig.cjs.json",
		"prepublishOnly": "pnpm build"
	},
	"peerDependencies": {
		"react": "^16.x || ^17.x || ^18.x || ^19.0.0 || ^19.0.0-rc"
	},
	"devDependencies": {
		"@repo/generate-icon-lib": "workspace:*",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"react": "^19.1.1",
		"tsdx": "^0.14.1",
		"typescript": "^5.9.2"
	}
}
