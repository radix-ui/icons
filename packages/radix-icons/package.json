{
	"name": "@radix-ui/react-icons",
	"version": "1.3.2",
	"license": "MIT",
	"description": "Radix UI React Icon Set",
	"source": "./src/index.tsx",
	"type": "module",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"files": [
		"outline/",
		"solid/",
		"dist/",
		"index.js",
		"manifest.json"
	],
	"sideEffects": false,
	"publishConfig": {
		"access": "public",
		"module": "dist/react-icons.esm.js",
		"main": "index.js",
		"types": "dist/index.d.ts"
	},
	"scripts": {
		"generate-src": "generate-icon-lib --file=uvCPWGs49VP4oslkOtVcCu && mv ./icons/15/* ./icons && rmdir ./icons/15 && cd ./icons && zip -r ../../../radix-icons.zip ./",
		"build": "rimraf dist && pnpm build:esm && pnpm build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --project tsconfig.cjs.json",
		"prepublishOnly": "pnpm build"
	},
	"peerDependencies": {
		"react": "^16.x || ^17.x || ^18.x || ^19.0.0 || ^19.0.0-rc"
	},
	"devDependencies": {
		"@repo/generate-icon-lib": "workspace:*",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"react": "^19.1.1",
		"tsdx": "^0.14.1",
		"typescript": "^5.9.2"
	}
}
